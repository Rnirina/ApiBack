<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Categorie</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Categorie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Etat</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Etat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Match</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Match</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Opposant</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Opposant</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pari</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Pari</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Participant</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Participant</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Statut</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Statut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeMatch</servlet-name>
        <servlet-class>mg.itu.tpt.relais.TypeMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TypeUtilisateur</servlet-name>
        <servlet-class>mg.itu.tpt.relais.TypeUtilisateur</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Utilisateur</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Utilisateur</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>mg.itu.tpt.relais.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Categorie</servlet-name>
        <url-pattern>/Categorie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Etat</servlet-name>
        <url-pattern>/Etat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Match</servlet-name>
        <url-pattern>/Match</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Opposant</servlet-name>
        <url-pattern>/Opposant</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pari</servlet-name>
        <url-pattern>/Pari</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Participant</servlet-name>
        <url-pattern>/Participant</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Statut</servlet-name>
        <url-pattern>/Statut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeMatch</servlet-name>
        <url-pattern>/TypeMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TypeUtilisateur</servlet-name>
        <url-pattern>/TypeUtilisateur</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Utilisateur</servlet-name>
        <url-pattern>/Utilisateur</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <filter>
        <filter-name>CORSFilter</filter-name>
        <filter-class>mg.itu.tpt.services.CorsFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CORSFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>
